{
	"compilerOptions": {
		"baseUrl": "./",
		"paths": {
			"components/*": ["src/components/*"],
			"stories/*": ["src/stories/*"]
		},
		"composite": true,
		"skipLibCheck": true,
		"module": "ESNext",
		"moduleResolution": "bundler",
		"allowSyntheticDefaultImports": true,
		"strict": true
	},
	"include": ["vite.config.ts"]
}
